<!DOCTYPE html>
<html>
  <head>
    <title>나의 게시판</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>나의 게시판</h1>
    <table>
      <tr>
        <th>NO.</th>
        <th>작성일자</th>
        <th>작성자</th>
        <th>제목</th>
        <th>조회수</th>
      </tr>
      <% BBS.forEach((vo,index) => { %>
        <tr data-seq="<%= vo.b_id %>">
          <th><%- index %></th>
          <th><%- vo.b_date %></th>
          <th><%- vo.b_time %></th>
          <th><%- vo.b_subject %></th>
          <th><%- vo.b_count %></th>
        </tr>
      <% })%>
      
    </table>
    <button class="btn_write">글쓰기</button>

  </body>
  <script>
    document.querySelector("button.btn_write").addEventListener("click",()=>{
      document.location.href = "/bbs/write";
    })
    document.querySelector("tr").addEventListener("click", (e) =>{
      
    })
  </script>
</html>
